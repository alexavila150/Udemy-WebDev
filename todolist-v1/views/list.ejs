<%- include("header") -%> 

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box"></div>
     
    <div class="box">
        <% items.forEach(element => { %>
        <div class="item">
            <input type="checkbox">
            <p><%= element  %></p>
        </div>
        <% }) %>

        <form action="/" method="post">
            <input type="text", name="todoItem" placeholder="New Item" autocomplete="off" required>
            <button type="submit" name="list" value=<%= listTitle %> >+</button>
        </form>

    </div>


<%- include("footer") -%> 